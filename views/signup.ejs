<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%- include ('partials/script'); -%> <%- include ('partials/head'); -%>
  </head>
  <body>
    <%- include ('partials/navbar'); -%>
    <div class="container gx-3 gy-2 align-items-center my-5 py-5 w-50">
      <form method="POST" action="/signup">
        <div class="row mb-3">
          <label class="col-sm-2 col-form-label">UserName:</label>
          <div class="col-sm-8">
            <input class="form-control" type="name" name="user" />
          </div>
        </div>
        <div class="row mb-3">
          <label class="col-sm-2 col-form-label">email :</label>
          <div class="col-sm-8">
            <input class="form-control" type="text" name="email" />
          </div>
        </div>
        <div class="row mb-3">
          <label class="col-sm-2 col-form-label">Password :</label>
          <div class="col-sm-8">
            <input class="form-control" type="password" name="pass" />
          </div>
        </div>
        <button type="submit" id="log" class="btn btn-default">Submit</button>
      </form>
    </div>
    <div class="container m-5 p-5"></div>
    <div class="container m-5 p-5"></div>

    <footer class="footer"><%- include ('partials/footer'); -%></footer>
  </body>
</html>
