<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%- include ('partials/head'); -%>
  </head>
  <body style="background-color: #f5f5f5">
    <%- include ('partials/navbar'); -%>
    <div id="demo" class="carousel slide" data-ride="carousel">
      <ul class="carousel-indicators">
        <li data-target="#theCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#theCarousel" data-slide-to="1"></li>
        <li data-target="#theCarousel" data-slide-to="2"></li>
      </ul>
      <div class="carousel-inner" style="height: 400px">
        <div
          class="carousel-item active"
          style="
            height: 400px;
            background-position: center;
            background-size: cover;
            background-image: url('images/pics/sun.webp');
          "
        >
          <div class="carousel-caption ml-auto mr-auto">
            <p><h1 >FAMOUS PAINTINGS</h1></p>
          </div>
        </div>
        <div
          class="carousel-item"
          style="
            height: 400px;
            background-position: center;
            background-size: cover;
            background-image: url('images/pics/aaa.jpg');
          "
        ></div>
        <div
          class="carousel-item"
          style="
            height: 400px;
            background-position: center;
            background-size: cover;
            background-image: url('images/pics/flower.jpg');
          "
        >
          <div class="carousel-caption ml-auto mr-auto">
            <p><h1 >NEW ADDED</h1></p>
          </div>
        </div>
      </div>
      <a class="carousel-control-prev" href="#demo" data-slide="prev">
        <span class="carousel-control-prev-icon"></span>
      </a>
      <a class="carousel-control-next" href="#demo" data-slide="next">
        <span class="carousel-control-next-icon"></span>
      </a>
    </div>

    <%- include('partials/gallery'); -%>

    <div class="container m-2 p-2"></div>

    <footer class="footer">
      <%- include ('partials/footer'); -%> <%- include ('partials/script'); -%>
    </footer>
  <!-- <script src="javascripts/javascript.js"></script> -->
  </body>
</html>
